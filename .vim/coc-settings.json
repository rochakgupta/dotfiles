{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },
  "suggest.maxCompleteItemCount": 10,
  "prettier.printWidth": 80,
  "explorer.git.enable": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.reveal.auto": true
}
