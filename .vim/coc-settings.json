{
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "html",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "python",
    "typescript",
    "typescriptreact"
  ],
  "diagnostic.checkCurrentLine": true,
  "dialog.maxWidth": 80,
  "explorer.buffer.root.template": "[icon & 1] BUFFERS",
  "explorer.file.reveal.auto": true,
  "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.file.showHiddenFiles": true,
  "explorer.git.enable": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.width": 30,
  "notification.disabledProgressSources": ["*"],
  "pairs.enableBackspace": false,
  "pairs.enableCharacters": ["{", "("],
  "prettier.printWidth": 80,
  "pyright.inlayHints.functionReturnTypes": true,
  "pyright.inlayHints.variableTypes": false,
  "suggest.maxCompleteItemCount": 10,
  "suggest.minTriggerInputLength": 3,
  "suggest.snippetsSupport": false
}
